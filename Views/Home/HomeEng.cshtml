<section class="section section--contact contact" id="contact">
    <div class="section__content">
        <div class="contact">

            <div class="container">
                <div class="row">

                    <div class="col-md-6">
                        <div class="contact__info">
                            <h3 class="contact__title">
                                Contact
                            </h3>
                            <p class="contact__description">
                                We’s love to share with you what we’ve up to each month and experience on our retail point of sales production services. We will never share your details with anyone outside. If you’re happy to hear from us please let us know!
                            </p>

                            <ul class="contact__addresses">
                                <li class="contact__address">
                                    Email: hung.nguyentien@lienphatad.com.vn
                                </li>
                                <li class="contact__address">
                                    Mr. Hung - Managing Director: 0938.685.869
                                </li>
                                <li class="contact__address">
                                    Email: thoi.nguyenvan@lienphatad.com.vn
                                </li>
                                <li class="contact__address">
                                    Mr. Thoi - Sales Manager: 093.8468.719
                                </li>
                            </ul>
                        </div>
                    </div>

                    <div class="col-md-6">
                        <h3 class="contact__title">
                            Subscribe
                        </h3>
                        <form id="contact-form">
                            <div class="contact__form">
                                <div class="contact__form-row js-contact-validate">
                                    <label for="name" class="contact__label">Name</label>
                                    <input type="text" name="Name" placeholder="" class="contact__input">
                                    <div class="contact__form-validate d-none"></div>
                                </div>
                                <div class="contact__form-row js-contact-validate">
                                    <label for="phone" class="contact__label">Phone number</label>
                                    <input type="tel" name="Phone" placeholder="" class="contact__input">
                                    <div class="contact__form-validate d-none"></div>
                                </div>
                                <div class="contact__form-row js-contact-validate">
                                    <label for="email" class="contact__label">Email address</label>
                                    <input type="email" name="Email" placeholder="" class="contact__input">
                                    <div class="contact__form-validate d-none"></div>
                                </div>
                                <div class="contact__form-row">
                                    <label for="message" class="contact__label">Your request</label>
                                    <textarea name="Message" id="" cols="30" rows="4" class="contact__input" placeholder="Please enter the content"></textarea>
                                </div>

                                <button type="submit" id="contact-submit" class="btn btn--primary">Get in touch</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

